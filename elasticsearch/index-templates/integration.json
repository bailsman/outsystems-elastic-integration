{
  "index_patterns": "os-mon-log-integration*",
  "version": 1,
  "settings": {
    "index.refresh_interval": "15s",
    "index.number_of_shards": "1",
    "index.lifecycle.name": "os-mon-log-ilm-policy",
    "index.lifecycle.rollover_alias": "os-mon-log-integration"
  },
  "mappings": {
    "properties": {
      "tenant_id": {
        "type": "long"
      },
      "request": {
        "properties": {
          "key": {
            "type": "keyword"
          }
        }
      },
      "import": {
        "properties": {
          "latency": {
            "type": "long"
          }
        }
      },
      "error_id": {
        "type": "keyword"
      },
      "log": {
        "properties": {
          "data_source": {
            "type": "keyword"
          },
          "instant": {
            "type": "date"
          }
        }
      },
      "outsystems": {
        "properties": {
          "location_name": {
            "type": "keyword"
          },
          "customer_name": {
            "type": "keyword"
          },
          "environment_name": {
            "type": "keyword"
          }
        }
      },
      "application_endpoint_name": {
        "type": "keyword"
      },
      "duration": {
        "type": "long"
      },
      "@timestamp": {
        "type": "date"
      },
      "application": {
        "properties": {
          "espace_id": {
            "type": "long"
          },
          "espace_name": {
            "type": "keyword"
          },
          "action_name": {
            "type": "keyword"
          },
          "name": {
            "type": "keyword"
          },
          "client_ip": {
            "type": "keyword"
          },
          "key": {
            "type": "keyword"
          }
        }
      },
      "ecs": {
        "properties": {
          "version": {
            "type": "keyword"
          }
        }
      },
      "@version": {
        "type": "keyword"
      },
      "integration": {
        "properties": {
          "executor": {
            "type": "keyword"
          },
          "id": {
            "type": "keyword"
          },
          "type": {
            "type": "keyword"
          },
          "expose": {
            "type": "boolean"
          }
        }
      },
      "duration_class": {
        "type": "keyword"
      }
    }
  }
}
