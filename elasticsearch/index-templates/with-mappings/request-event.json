{
  "index_patterns": "os-mon-log-request-event*",
  "version": 1,
  "settings": {
    "index.refresh_interval": "15s",
    "index.number_of_shards": "1",
    "index.lifecycle.name": "os-mon-log-ilm-policy",
    "index.lifecycle.rollover_alias": "os-mon-log-request-event"
  },
  "mappings": {
    "properties": {
      "request": {
        "properties": {
          "error_count": {
            "type": "long"
          },
          "time_to_last_byte": {
            "type": "long"
          },
          "time_to_first_byte": {
            "type": "long"
          },
          "extension": {
            "properties": {
              "total_time": {
                "type": "long"
              },
              "total_executions": {
                "type": "long"
              },
              "total_time_class": {
                "type": "keyword"
              }
            }
          },
          "query": {
            "properties": {
              "total_time": {
                "type": "long"
              },
              "total_executions": {
                "type": "long"
              },
              "total_time_class": {
                "type": "keyword"
              }
            }
          },
          "integration": {
            "properties": {
              "total_time": {
                "type": "long"
              },
              "total_executions": {
                "type": "long"
              },
              "total_time_class": {
                "type": "keyword"
              }
            }
          },
          "server_duration": {
            "type": "long"
          },
          "total_duration": {
            "type": "long"
          },
          "load_time": {
            "type": "long"
          },
          "total_duration_class": {
            "type": "keyword"
          },
          "key": {
            "type": "keyword"
          },
          "event": {
            "properties": {
              "details": {
                "type": "text",
                "fields": {
                  "keyword": {
                    "ignore_above": 256,
                    "type": "keyword"
                  }
                }
              },
              "name": {
                "type": "keyword"
              }
            }
          }
        }
      },
      "import": {
        "properties": {
          "latency": {
            "type": "long"
          }
        }
      },
      "log": {
        "properties": {
          "data_source": {
            "type": "keyword"
          },
          "instant": {
            "type": "date"
          }
        }
      },
      "session": {
        "properties": {
          "size": {
            "type": "long"
          },
          "visit_key": {
            "type": "keyword"
          },
          "acquisition_time": {
            "type": "long"
          },
          "viewstate_size": {
            "type": "long"
          },
          "viewstate_size_class": {
            "type": "keyword"
          },
          "size_class": {
            "type": "keyword"
          },
          "visitor_key": {
            "type": "keyword"
          },
          "key": {
            "type": "keyword"
          }
        }
      },
      "user_key": {
        "type": "keyword"
      },
      "outsystems": {
        "properties": {
          "location_name": {
            "type": "keyword"
          },
          "customer_name": {
            "type": "keyword"
          },
          "environment_name": {
            "type": "keyword"
          }
        }
      },
      "@timestamp": {
        "type": "date"
      },
      "application": {
        "properties": {
          "tenant_name": {
            "type": "keyword"
          },
          "tenant_key": {
            "type": "keyword"
          },
          "endpoint_key": {
            "type": "keyword"
          },
          "endpoint_name": {
            "type": "keyword"
          },
          "module_key": {
            "type": "keyword"
          },
          "screen_resolution": {
            "type": "keyword"
          },
          "action_key": {
            "type": "keyword"
          },
          "environment": {
            "properties": {
              "name": {
                "type": "keyword"
              },
              "key": {
                "type": "keyword"
              }
            }
          },
          "action_name": {
            "type": "keyword"
          },
          "name": {
            "type": "keyword"
          },
          "frontend_name": {
            "type": "keyword"
          },
          "client_ip": {
            "type": "keyword"
          },
          "module_name": {
            "type": "keyword"
          },
          "key": {
            "type": "keyword"
          }
        }
      },
      "ecs": {
        "properties": {
          "version": {
            "type": "keyword"
          }
        }
      },
      "user_id": {
        "type": "long"
      },
      "@version": {
        "type": "keyword"
      },
      "user_agent": {
        "type": "text",
        "fields": {
          "keyword": {
            "ignore_above": 256,
            "type": "keyword"
          }
        }
      }
    }
  }
}
