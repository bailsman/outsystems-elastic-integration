{
  "index_patterns": "os-mon-log-web-reference*",
  "version": 1,
  "settings": {
    "index.refresh_interval": "15s",
    "index.number_of_shards": "1",
    "index.lifecycle.name": "os-mon-log-ilm-policy",
    "index.lifecycle.rollover_alias": "os-mon-log-web-reference"
  },
  "mappings": {
    "properties": {
      "tenant_id": {
        "type": "long"
      },
      "import": {
        "properties": {
          "latency": {
            "type": "long"
          }
        }
      },
      "error_id": {
        "type": "keyword"
      },
      "log": {
        "properties": {
          "data_source": {
            "type": "keyword"
          },
          "instant": {
            "type": "date"
          }
        }
      },
      "web_reference": {
        "properties": {
          "url": {
            "type": "text",
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            }
          }
        }
      },
      "outsystems": {
        "properties": {
          "location_name": {
            "type": "keyword"
          },
          "customer_name": {
            "type": "keyword"
          },
          "environment_name": {
            "type": "keyword"
          }
        }
      },
      "duration": {
        "type": "long"
      },
      "@timestamp": {
        "type": "date"
      },
      "application": {
        "properties": {
          "espace_id": {
            "type": "long"
          },
          "method_name": {
            "type": "keyword"
          },
          "executor": {
            "type": "keyword"
          }
        }
      },
      "ecs": {
        "properties": {
          "version": {
            "type": "keyword"
          }
        }
      },
      "@version": {
        "type": "keyword"
      },
      "integration": {
        "properties": {
          "id": {
            "type": "keyword"
          }
        }
      },
      "duration_class": {
        "type": "keyword"
      }
    }
  }
}
